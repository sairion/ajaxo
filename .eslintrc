parser : "babel-eslint"
globals:
    sinon: true
env:
    node: true
    browser: true
    mocha: true
rules:
    camelcase: 0
    comma-dangle: 0
    consistent-return: 0
    curly: 0
    dot-notation: 0
    eqeqeq: 0
    strict: 0
    new-cap: 0
    no-underscore-dangle: 0
    no-console: 1
    no-unused-vars: [2, {"vars": "all", "args": "none"}]
    quotes: 0
